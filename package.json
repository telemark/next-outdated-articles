{
  "name": "next-outdated-articles",
  "description": "Solution for locating outdated articles",
  "version": "1.4.6",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "engines": {
    "node": ">=10.14.2"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "dev": "next",
    "build": "next build && next export",
    "now-build": "next build && next export",
    "start": "next start",
    "deploy": "npm test && now && now alias",
    "cleanup": "rm -rf .next && rm -rf out"
  },
  "keywords": [
    "web quality",
    "next",
    "outdated articles",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/next-outdated-articles.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/next-outdated-articles/issues"
  },
  "homepage": "https://github.com/telemark/next-outdated-articles#readme",
  "devDependencies": {
    "ava": "1.4.1",
    "babel-eslint": "10.0.1",
    "coveralls": "3.0.3",
    "nyc": "14.0.0",
    "standard": "12.0.1"
  },
  "dependencies": {
    "next": "8.1.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "axios": "0.18.0",
    "muicss": "0.9.41"
  },
  "standard": {
    "ignore": [
      "/static/*.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  }
}
